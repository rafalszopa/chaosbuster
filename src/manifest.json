{
  "name": "chaosbuster",
  "version": "1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/assets/icons/chrome/16.png",
      "32": "/assets/icons/chrome/32.png",
      "48": "/assets/icons/chrome/48.png",
      "128": "/assets/icons/chrome/128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
